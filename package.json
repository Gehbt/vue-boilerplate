    "npm-run-all2": "^6.2.2",
    "prettier": "^3.3.2",
    "simple-git-hooks": "^2.11.1",
    "typescript": "~5.5.3",
    "typescript-eslint": "^7.15.0",
    "vite": "^5.3.3",
    "vite-plugin-checker": "^0.7.1",
    "vite-plugin-vue-devtools": "^7.3.5",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.26"
  },
  "simple-git-hooks": {
    "pre-push": "pnpm run lint",
