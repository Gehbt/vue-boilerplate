{
  "name": "vue-boilerplate",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test": "vitest run",
    "build-only": "vite build -c vite.config.build.ts",
    "type-check": "vue-tsc --build --force",
    "lint": "run-s lint.format lint.fix",
    "lint.fix": "eslint --fix",
    "lint.format": "prettier --write src/"
  },
  "dependencies": {
    "@iconify/vue": "^4.1.2",
    "@picocss/pico": "^2.0.6",
    "oxc": "^1.0.1",
    "pinia": "^2.1.7",
